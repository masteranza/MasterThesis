title = {{Understanding Atmospheric Behaviour in Terms of Entropy: A Review of Applications of the Second Law of Thermodynamics to Meteorology}},
journal = {Entropy},
year = {2011},
volume = {13},
number = {12},
pages = {211--240},
month = dec,
publisher = {Molecular Diversity Preservation International},
