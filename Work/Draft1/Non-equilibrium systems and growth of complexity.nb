 FormBox[
  RowBox[{
   RowBox[{"H", "(", 
    RowBox[{"p", "||", "q"}], ")"}], "=", 
   RowBox[{"-", 
    RowBox[{
     UnderscriptBox["\[Sum]", "i"], 
     RowBox[{
      SubscriptBox["p", "i"], "ln", " ", 
      FractionBox[
       SubscriptBox["p", "i"], 
       SubscriptBox["q", "i"]]}]}]}]}], TraditionalForm]], "EquationNumbered",\

