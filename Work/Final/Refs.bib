@article{Kadanoff:2009bg,
author = {Kadanoff, Leo P},
title = {{More is the Same; Phase Transitions and Mean Field Theories}},
journal = {arXiv.org},
year = {2009},
eprint = {0906.0653},
eprinttype = {arxiv},
eprintclass = {physics.hist-ph},
number = {5-6},
pages = {777--797},
