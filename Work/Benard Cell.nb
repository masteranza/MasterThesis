Cell[TextData[{
 "However if consider an equation in which ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["j", "e"], "\[NotEqual]", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " then we get a different solution"
}], "Text",
 CellChangeTimes->{
  3.6962357391589737`*^9, {3.696235772064054*^9, 3.6962358009993267`*^9}},
 CellID->1374481012],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "[", "t", "]"}], "\[Equal]", 
    RowBox[{"je", "+", 
     FractionBox[
      RowBox[{"y0", "-", 
       RowBox[{"y", "[", "t", "]"}]}], "\[Tau]"]}]}], ",", 
   RowBox[{"y", "[", "t", "]"}], ",", "t"}], "]"}]], "Input",
 CellChangeTimes->{{3.69623575385466*^9, 3.696235758949147*^9}},
 CellID->735017959],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"y", "[", "t", "]"}], "\[Rule]", 
    RowBox[{"y0", "+", 
     RowBox[{"je", " ", "\[Tau]"}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", 
        FractionBox["t", "\[Tau]"]}]], " ", 
      RowBox[{"C", "[", "1", "]"}]}]}]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{3.696235759774695*^9},
 CellID->577364602]
}, Open  ]],

