Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "[", "t", "]"}], "\[Equal]", 
    RowBox[{"je", "+", 
     FractionBox[
      RowBox[{"y0", "-", 
       RowBox[{"y", "[", "t", "]"}]}], "\[Tau]"]}]}], ",", 
   RowBox[{"y", "[", "t", "]"}], ",", "t"}], "]"}]], "Input",
