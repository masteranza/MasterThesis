    RowBox[{"y", "[", "t", "]"}], "\[Rule]", 
    RowBox[{"y0", "+", 
     RowBox[{"je", " ", "\[Tau]"}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", 
