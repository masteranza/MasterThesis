Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["W", "d"], "[", "x", "]"}], "=", 
   RowBox[{
    RowBox[{"W", "[", "x", "]"}], "-", 
    SubscriptBox["W", "r"]}]}], TraditionalForm]], "EquationNumbered",
