Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[Pi]", "(", 
     RowBox[{"II", "\[Rule]", "I"}], ")"}], 
    RowBox[{"\[Pi]", "(", 
     RowBox[{"I", "\[Rule]", "II"}], ")"}]], "=", 
   RowBox[{
    SubscriptBox[
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox[
       RowBox[{"\[LeftAngleBracket]", 
        SuperscriptBox["\[ExponentialE]", 
