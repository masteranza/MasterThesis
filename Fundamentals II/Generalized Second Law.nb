  ButtonBox[
   RowBox[{"(", 
    CounterBox["Section", "MacroscopicLaw"], ".", 
    CounterBox["EquationNumbered", "MacroscopicLaw"], ")"}],
   BaseStyle->{"Hyperlink", Larger},
