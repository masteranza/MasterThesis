Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[Pi]", "(", 
     RowBox[{"II", "\[Rule]", "I"}], ")"}], 
    RowBox[{"\[Pi]", "(", 
     RowBox[{"I", "\[Rule]", "II"}], ")"}]], "=", 
   SubscriptBox[
    RowBox[{"\[LeftAngleBracket]", 
     SubscriptBox[
      RowBox[{"\[LeftAngleBracket]", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", 
         SubscriptBox["\[Beta]\[CapitalDelta]S", "tot"]}]], 
       "\[RightAngleBracket]"}], 
      RowBox[{"i", "\[Rule]", "j"}]], "\[RightAngleBracket]"}], 
    RowBox[{"I", "\[Rule]", "II"}]]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.6942853075807323`*^9, 3.694285349158513*^9}, {
  3.694362681160613*^9, 3.694362686776557*^9}},
 CellTags->"MacroscopicLaw2",
 CellID->971827874],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalDelta]S", "tot"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "ln"}], 
      FractionBox[
       RowBox[{" ", 
        RowBox[{"p", "(", 
         RowBox[{"j", "\[VerticalSeparator]", "II"}], ")"}]}], 
       RowBox[{"p", "(", 
        RowBox[{"i", "\[VerticalSeparator]", "II"}], ")"}]]}], "+", 
     RowBox[{"\[Beta]", "\[VeryThinSpace]", 
      SubscriptBox["\[CapitalDelta]Q", "ij"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " where the first term is the internal entropy change, the second external \
also called dissipation."
}], "Text",
 CellChangeTimes->{{3.6942853536430473`*^9, 3.694285488771062*^9}},
 CellID->511454276],

Cell[TextData[{
 "It is sometimes useful to simplify the notation and write ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"\[LeftAngleBracket]", "\[RightAngleBracket]"}], 
