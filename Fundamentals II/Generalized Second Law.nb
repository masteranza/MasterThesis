 CellID->43948817],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"ln", "[", 
    FractionBox[
     RowBox[{"\[Pi]", "(", 
      RowBox[{"I", "\[Rule]", "II"}], ")"}], 
